{
  "name": "extended-parse-duration",
  "version": "0.2.0",
  "description": "Convert a string of duration (colon notation, ms format, etc) to milliseconds.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "ava test.ts",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "pubdry": "pnpm publish --dry-run --no-git-checks"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "environmentVariables": {
      "TS_NODE_LOG_ERROR": "true"
    },
    "require": [
      "ts-node/register"
    ]
  },
  "keywords": [
    "time",
    "parse",
    "duration",
    "ms"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "ava": "^5.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "ms": "^2.1.3",
    "parse-duration": "^1.0.3"
  },
  "author": {
    "name": "Hizkia Ray",
    "email": "personal@13373333.one",
    "url": "https://13373333.one"
  },
  "repository": "https://github.com/ray-1337/extended-parse-duration.git",
  "homepage": "https://github.com/ray-1337/extended-parse-duration#readme",
  "engines": {
    "node": ">=16"
  }
}
